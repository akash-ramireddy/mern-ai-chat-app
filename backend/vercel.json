{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "configs/**",
          "routes/**",
          "controllers/**",
          "models/**",
          "middlewares/**"
        ]
      }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "server.js" }
  ]
}